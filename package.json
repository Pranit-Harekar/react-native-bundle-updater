{
  "name": "ReactNativeBundleUpdater",
  "version": "0.0.1",
  "description": "A client side library for dynamically updating jsbundles in React Native apps",
  "author": "Pranit Harekar <pranitharekar@hotmail.com>",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "start": "yarn react-native start",
    "ios": "yarn react-native run-ios",
    "android": "yarn react-native run-android",
    "test": "jest",
    "lint": "eslint .",
    "bundle": "react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ios/main.jsbundle  --assets-dest ios"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.3.3",
    "jest-haste-map": "^24.0.0",
    "jest-resolve": "^24.1.0",
    "react": "16.8.6",
    "react-native": "0.62.3",
    "semver": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/runtime": "^7.4.2",
    "@react-native-community/eslint-config": "^0.0.3",
    "@types/jest": "^24.0.11",
    "@types/react": "^16.8.13",
    "@types/react-native": "^0.57.43",
    "@types/react-test-renderer": "^16.8.1",
    "@types/semver": "^6.0.0",
    "babel-jest": "^24.5.0",
    "eslint": "^5.16.0",
    "jest": "^24.5.0",
    "metro-react-native-babel-preset": "^0.53.1",
    "prettier": "^1.16.4",
    "react-test-renderer": "16.8.3",
    "typescript": "^3.4.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
